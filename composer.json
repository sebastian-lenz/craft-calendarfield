{
  "name": "sebastianlenz/calendarfield",
  "description": "A Craft field type for storing event data",
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "craftcms/cms": "^5.0.0",
    "eluceo/ical": "^0.15.0",
    "rlanvin/php-rrule": "^2.2",
    "sebastianlenz/craft-utils": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "lenz\\calendarfield\\": "src/"
    }
  },
  "extra": {
    "handle": "calendarfield",
    "name": "Calendar field",
    "developer": "Sebastian Lenz",
    "developerUrl": "https://github.com/sebastian-lenz/"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
